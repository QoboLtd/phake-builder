{
	"name": "qobo/phake-builder",
	"description": "A set of build and deploy files, based on jaz303/phake",
	"license": "GPL-2.0",
	"authors": [
		{
			"name": "Leonid Mamchenkov",
			"email": "leonid@mamchenkov.net",
			"homepage": "http://mamchenkov.net",
			"role": "Developer"
		}
	],
	"require": {
		"jaz303/phake": "~0.6",
		"vlucas/phpdotenv": "~1.1",
		"interconnectit/search-replace-db": "3.1",
		"qobo/pattern": "~1.0",
		"monolog/monolog": "~1.11",
		"kmelia/monolog-stdout-handler": "~1.2",
		"zetacomponents/archive": "1.5",
		"heartsentwined/file-system-manager": "~1.1",
		"symfony/filesystem": "~2.5",
		"gorkalaucirica/hipchat-v2-api-client": "~1.5"
	},
	"autoload": {
		"psr-4": {"PhakeBuilder\\": "src/"}
	},
	"autload-dev": {
		"psr-4": {"PhakeBuilder\\Tests\\": "tests/"}
	},
	"require-dev": {
		"phpunit/phpunit": "~4.3",
		"squizlabs/php_codesniffer": "~2.5"
	}
}
